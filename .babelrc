{
    "presets": ["env", "es2015", "react", "stage-2"],
    "plugins": [[
        "react-css-modules", {
            "context": "./app/components",
            "filetypes": {
                ".scss": {
                    "syntax": "postcss-scss"
                }
            },
            "generateScopedName": "[path]___[name]__[local]___[hash:base64:5]"
        }, "transform-object-rest-spread"
    ]]
}